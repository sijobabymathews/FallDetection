function [if_fall ] = get_if_fall(M)
%get_if_fall
% input: 
%   M -> dataset from csv file
[Ax, Ay, Az] = get_accelerations(M);
mag = get_mag(Ax, Ay, Az);
jerk = get_jerk(mag);
[Px, Py, Pz] = get_change_in_orientation(M);
P_mag = get_mag(Px, Py, Pz);

% get max jerk and p_mag


[max_jerk, t1] = max(jerk);
[max_p, t2] = max(P_mag);
% comparitors
if (max_jerk>= 0.1) && (max_p>=212) && (t1 == t2)
    if_fall = true;
else
    if_fall = false;
end